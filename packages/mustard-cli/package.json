{
  "name": "mustard-cli",
  "version": "0.1.2",
  "main": "./dist/source/Exports/index.js",
  "types": "./dist/source/Exports/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "local": "nodemon index.ts run sync --dry",
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "pnpm run build"
  },
  "nodemonConfig": {
    "delay": 500,
    "env": {
      "NODE_ENV": "development"
    },
    "execMap": {
      "ts": "ts-node-esm"
    },
    "ext": "ts,json",
    "ignore": [
      "**/test/**",
      "**/docs/**",
      "node_modules"
    ],
    "restartable": "rs",
    "verbose": true,
    "watch": [
      "*.ts"
    ]
  },
  "dependencies": {
    "fastest-levenshtein": "^1.0.16",
    "find-up": "^6.3.0",
    "mri": "^1.2.0",
    "yargs-parser": "^21.1.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@types/yargs-parser": "^21.0.0",
    "chalk": "4.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "npm:@typescript-deploys/pr-build@5.0.0-pr-50820-31"
  }
}