{
  "name": "mustard-sample",
  "private": true,
  "scripts": {
    "local": "nodemon index.ts"
  },
  "nodemonConfig": {
    "delay": 500,
    "env": {
      "NODE_ENV": "development"
    },
    "execMap": {
      "ts": "ts-node-esm"
    },
    "ext": "ts,json",
    "ignore": [
      "**/test/**",
      "**/docs/**",
      "node_modules"
    ],
    "restartable": "rs",
    "verbose": true,
    "watch": [
      "*.ts"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "chalk": "^4.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "npm:@typescript-deploys/pr-build@5.0.0-pr-50820-31"
  },
  "dependencies": {
    "mustard-cli": "workspace:*"
  }
}